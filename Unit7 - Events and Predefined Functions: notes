import keyEvents
import random
import turtle

# random colored square on click
'''
screen = turtle.Screen()
screen.bgcolor("black")
turtle.speed(0)

colors = ["red", "green", "blue", "yellow", "magenta", "cyan",
"orange", "purple", "pink"]

# defining a function
def makeSquare(x, y):
  turtle.color(random.choice(colors))
  turtle.penup()
  turtle.setpos(x, y)
  turtle.pendown()
  for i in range(4):
    turtle.forward(100)
    turtle.left(90)

# invoking the defined function    
screen.onclick(makeSquare)
