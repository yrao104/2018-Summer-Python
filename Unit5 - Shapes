import turtle

for x in range(4):
  turtle.forward(20)
  turtle.right(90)
for x in range(6):
  turtle.color("red")
  turtle.forward(35)
  turtle.right(60)
for x in range(8):
  turtle.color("blue")
  turtle.forward(50)
  turtle.right(45)
for x in range(10):
  turtle.color("green")
  turtle.forward(55)
  turtle.right(36)
